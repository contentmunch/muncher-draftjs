{
  "name": "@contentmunch/muncher",
  "version": "0.5.8",
  "description": "Content editor wysiwyg for contentmunch",
  "author": "asikpradhan",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/contentmunch/muncher.git"
  },
  "scripts": {
    "start": "npm run storybook",
    "prebuild": "rimraf lib",
    "build": "npm run build:types && npm run build:js ",
    "postbuild": "rsync -avz --exclude={\"*.ts\",\"*.tsx\",\"__snapshots__\",\"*.js\",\"*.mdx\"} src/ lib/",
    "build:types": "cp tsconfig-tsc.json tsconfig.json && tsc --emitDeclarationOnly",
    "postbuild:types": "rimraf lib/**/*.test.d.ts lib/*.stories.d.ts lib/**/*.stories.d.ts lib/setupTests.d.ts",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx,.js\" --source-maps inline",
    "prepublishOnly": "npm run build",
    "pretest": "cp tsconfig-test.json tsconfig.json",
    "test": "react-scripts test",
    "storybook": "start-storybook --no-dll -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@contentmunch/muncher-ui": "^0.5.8",
    "@storybook/addon-actions": "^6.0.27",
    "@storybook/addon-essentials": "^6.0.27",
    "@storybook/addon-links": "^6.0.27",
    "@storybook/addon-storyshots": "^6.0.27",
    "@storybook/node-logger": "^6.0.27",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.27",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/draft-convert": "^2.1.1",
    "@types/draft-js": "^0.10.44",
    "@types/jest": "^26.0.15",
    "@types/js-beautify": "^1.11.0",
    "@types/node": "^12.19.1",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "babel-loader": "^8.1.0",
    "codemirror": "^5.58.2",
    "draft-convert": "^2.1.10",
    "draft-js": "^0.11.7",
    "js-beautify": "^1.13.0",
    "react": "^17.0.1",
    "react-codemirror2": "^7.2.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "react-json-tree": "^0.13.0",
    "react-loading-skeleton": "^2.1.1",
    "react-scripts": "4.0.0",
    "react-test-renderer": "^17.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "node-sass": "^4.14.1"
  }
}
